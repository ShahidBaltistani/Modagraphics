
@{
    ViewBag.Title = "Approval Manager";
    ViewBag.MainMenu = "mDashboard";
    ViewBag.SubMenu = "sDashboard";
}
@section controller {
    <script src="~/Angular/controllers/dashboard/approvalmanager.js"></script>
}

<div ng-app="app">
    <div ng-controller="approvalmanager">
        <!--Begin::Section-->
        <div class="row">
            <div class="col-xl-6">
                <!--begin:: Widgets/Top Products-->
                <div class="m-portlet m-portlet--bordered-semi m-portlet--full-height  m-portlet--rounded">
                    <div class="m-portlet__head">
                        <div class="m-portlet__head-caption">
                            <div class="m-portlet__head-title">
                                <h3 class="m-portlet__head-text">
                                    Approval Manager
                                </h3>
                            </div>
                        </div>
                    </div>
                    <div class="m-portlet__body">
                        <!--begin::Widget5-->
                        <div class="m-widget4">
                            <div class="m-widget4__chart m-portlet-fit--sides m--margin-top-10 m--margin-top-20" style="height:260px;">
                                <canvas id="m_chart_trends_stats"></canvas>
                            </div>
                            <div class="m-widget4__item">
                                <div class="m-widget4__img m-widget4__img--logo">
                                    <i class="fa fa-user"></i>
                                    <!--<img src="~/Theme/img/client-logos/logo3.png" alt="">-->
                                </div>
                                <div class="m-widget4__info">
                                    <span class="m-widget4__title">
                                        Users Approval
                                    </span>
                                </div>
                                <span class="m-widget4__ext">
                                    <span class="m-widget4__number m--font-danger" ng-model="UsersApprovals">
                                        {{UsersApprovals}}
                                    </span>
                                </span>
                            </div>
                            <div class="m-widget4__item">
                                <div class="m-widget4__img m-widget4__img--logo">
                                    <i class="fa fa-user"></i>
                                    <!-- <img src="~/Theme/img/client-logos/logo1.png" alt="">-->
                                </div>
                                <div class="m-widget4__info">
                                    <span class="m-widget4__title">
                                        Job Approvals
                                    </span>
                                </div>
                                <span class="m-widget4__ext">
                                    <span class="m-widget4__number m--font-danger" ng-model="JobApprovals">
                                        {{JobApprovals}}
                                    </span>
                                </span>
                            </div>
                        </div>
                        <!--end::Widget 5-->
                    </div>
                </div>
                <!--end:: Widgets/Top Products-->
            </div>
            <div class="col-xl-6">
                <!--begin:: Widgets/Activity-->
                <div class="m-portlet m-portlet--bordered-semi m-portlet--widget-fit m-portlet--full-height m-portlet--skin-light  m-portlet--rounded m-portlet--rounded-force">
                    <div class="m-portlet__head">
                        <div class="m-portlet__head-caption">
                            <div class="m-portlet__head-title">
                                <h3 class="m-portlet__head-text m--font-light">
                                    Jobs Detail
                                </h3>
                            </div>
                        </div>
                    </div>
                    <div class="m-portlet__body">
                        <div class="m-widget17">
                            <div class="m-widget17__visual m-widget17__visual--chart m-portlet-fit--top m-portlet-fit--sides m--bg-danger">
                                <div class="m-widget17__chart" style="height:320px;">
                                    <canvas id="m_chart_activities"></canvas>
                                </div>
                            </div>
                            <div class="m-widget17__stats">
                                <div class="m-widget17__items m-widget17__items-col1">
                                    <div class="m-widget17__item">
                                        <span class="m-widget17__icon">
                                            <i class="fa fa-user-circle"></i>
                                        </span>
                                        <span class="m-widget17__subtitle" ng-model="UsersApprovals">
                                            {{UsersApprovals}}
                                        </span>
                                        <span class="m-widget17__desc">
                                            Users to be Approved
                                        </span>
                                    </div>
                                    <div class="m-widget17__item">
                                        <span class="m-widget17__icon">
                                            <i class="fa fa-exclamation-triangle"></i>
                                        </span>
                                        <span class="m-widget17__subtitle">
                                            {{JobApprovals}}
                                        </span>
                                        <span class="m-widget17__desc">
                                            Jobs to be Approved
                                        </span>
                                    </div>
                                </div>
                                <div class="m-widget17__items m-widget17__items-col2">
                                    <div class="m-widget17__item">
                                        <span class="m-widget17__icon">
                                            <i class="fa fa-subway"></i>
                                        </span>
                                        <span class="m-widget17__subtitle">
                                            0
                                        </span>
                                        <span class="m-widget17__desc">
                                            Fleet owner to be approved
                                        </span>
                                    </div>
                                    <div class="m-widget17__item">
                                        <span class="m-widget17__icon">
                                            <i class="fa fa-spinner"></i>
                                        </span>
                                        <span class="m-widget17__subtitle">
                                            0
                                        </span>
                                        <span class="m-widget17__desc">
                                            Installer to be approved
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--end:: Widgets/Activity-->
            </div>
        </div>
        <!--End::Section-->
    </div>
</div>

<script src="~/Theme/js/dashboard.js" type="text/javascript"></script>
